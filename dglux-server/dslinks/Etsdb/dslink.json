{
  "name": "dslink-java-etsdb",
  "version": "0.19.3",
  "description": "Historian DSLink implementation for ETSDB",
  "license": "Apache",
  "author": {
    "name": "Samuel Grenier",
    "email": "samrg472@gmail.com"
  },
  "main": "bin/dslink-java-etsdb",
  "repository": {
    "type": "git",
    "url": "https://github.com/IOT-DSA/dslink-java-etsdb"
  },
  "bugs": {
    "url": "https://github.com/IOT-DSA/dslink-java-etsdb/issues"
  },
  "configs": {
    "name": {
      "type": "string",
      "default": "etsdb"
    },
    "broker": {
      "type": "url"
    },
    "token": {
      "type": "string"
    },
    "nodes": {
      "type": "path",
      "default": "nodes.json"
    },
    "key": {
      "type": "path",
      "default": ".key"
    },
    "log": {
      "type": "enum",
      "default": "info"
    },
    "handler_class": {
      "type": "string",
      "default": "org.dsa.iot.etsdb.Main"
    }
  }
}
